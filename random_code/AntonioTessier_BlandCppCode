#include <iostream>

void fill(int a[], int x) {
    for (int i = 0; i < x; ++i) {
        a[i] = i * 5;
    }
}

int main() {
    const int x = 10;
    int a[x];

    fill(a, x);

    for (int i = 0; i < x; ++i) {
        std::cout << a[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
